# <file system> <mount point>   <type>  <options>               <dump>  <pass>
/dev/sda1       /               ext3    defaults,noatime,nodiratime     1 1
proc            /proc           proc    defaults                        0 0
none            /dev/pts        devpts  gid=5,mode=20			0 0
/dev/sda2	swap		swap	defaults			0 0
